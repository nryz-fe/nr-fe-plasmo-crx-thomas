      "prettier --write"
    ]
  },
  "dependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "antd": "^4.21.3",
    "axios": "^0.27.2",
    "blueimp-md5": "^2.19.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "lodash-es": "^4.17.21",
    "qs": "^6.10.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@parcel/transformer-less": "^2.6.1",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/chrome": "0.0.188",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "17.0.40",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "plasmo": "latest",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "tailwindcss": "^3.1.4",
    "typescript": "4.7.3"
  },
