{
  "name": "nr-fe-plasmo-crx-thomas",
  "displayName": "Nr fe plasmo crx thomas",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "thomas",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx "
